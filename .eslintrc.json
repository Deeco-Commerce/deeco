{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier"],
  // "extends": ["prettier", "plugin:@typescript-eslint/recommended", "eslint:recommended"],

  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  // "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "endOfLine": "auto"
      }
    ],
    "no-console": "off",
    "no-alert": "off",
    "no-unused-vars": "off",
    "camelcase": "off",
    "no-html-link-for-pages": "off",
    "import/prefer-default-export": "off"
  }
}
